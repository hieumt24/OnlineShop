@model OnlineShop.Models.Db.Product
@{
    List<ProductGalery>? gallery = new List<ProductGalery>();
    if (ViewData["gallery"] != null)
    {
        gallery = ViewData["gallery"] as List<ProductGalery>;
    }
}
<!--breadcrumbs area start-->
<div class="breadcrumbs_area breadcrumbs_product mt-5">
    <div class="container">
        <div class="row">
            <div class="col-12">
                <div class="breadcrumb_content">
                    <ul>
                        <li><a href="/">home</a></li>
                        <li><a href="/products">shop</a></li>
                        <li>@Model.Title</li>
                    </ul>
                </div>
            </div>
        </div>
    </div>
</div>
<!--breadcrumbs area end-->

<!--product details start-->
<section class="product_details mb-135">
    <div class="container">
        <div class="row">
            <div class="col-lg-6 col-md-6">
                <div class="product_zoom_gallery">
                    <div class="zoom_gallery_inner d-flex">
                        <div class="zoom_tab_img">
                            <a class="zoom_tabimg_list" href="javascript:void(0)">
                                <img src="/images/banners/@Model.ImageName" alt="@Model.ImageName">
                            </a>
                            @if (gallery.Any())
                            {
                                @foreach (var item in gallery.OrderBy(x => x.Id))
                                {
                                    <a class="zoom_tabimg_list" href="javascript:void(0)">
                                        <img src="/images/banners/@item.ImageName" alt="@item.ImageName">
                                    </a>
                                }
                            }
                        </div>
                        <div class="product_zoom_main_img">
                            <div class="product_zoom_thumb">
                                <img data-image="/images/banners/@Model.ImageName"
                                     src="/images/banners/@Model.ImageName" alt="@Model.ImageName">
                            </div>
                            @if (gallery.Any())
                            {
                                @foreach (var item in gallery.OrderBy(x => x.Id))
                                {
                                    <div class="product_zoom_thumb">
                                        <img data-image="/images/banners/@item.ImageName"
                                             src="/images/banners/@item.ImageName" alt="@item.ImageName">
                                    </div>
                                }
                            }
                        </div>
                    </div>
                </div>
            </div>
            <div class="col-lg-6 col-md-6">
                <div class="product_d_right">
                    <form action="#">
                        <h1>@Model.Title</h1>
                        <div class="product_ratting_review d-flex align-items-center">

                            <div class="product_review">
                                <ul class="d-flex">
                                    <li>4 reviews</li>
                                    <li>Write your review</li>
                                </ul>
                            </div>
                        </div>
                        <div class="price_box">
                                <span class="current_price">
                                     @if (@Model.Discount != null)
                                    {
                                        <span class="current_price">
                                                @{
                                                decimal? discountedPrice;
                                                discountedPrice = @Model.Price.GetValueOrDefault(0) - @Model.Discount.GetValueOrDefault(0);
                                            }
                                            $@discountedPrice.GetValueOrDefault(0).ToString("F")
                                            </span>
                                        <span class="old_price">$@Model.Price.GetValueOrDefault(0).ToString("F")</span>
                                    }
                                    else
                                    {
                                        <span
                                            class="current-price">@Model.Price.GetValueOrDefault(0).ToString("F")</span>
                                    }
                                </span>
                        </div>
                        <div class="product_availalbe">
                            <ul class="d-flex">
                                <li><i class="icon-layers icons"></i> Only <span>
                                        @Model.Qty
                                    </span> left
                                </li>
                                <li>Availalbe:
                                    @if (Model.Qty > 0)
                                    {
                                        <span class="stock">In Stock</span>
                                    }
                                    else
                                    {
                                        <span class="text-danger">Out of Stock</span>
                                    }
                                </li>
                            </ul>
                        </div>
                        <div class="product_desc">
                            <p>@Model.Description </p>
                        </div>
                        <div class="product_variant">

                            <div class="variant_quantity_btn d-flex">
                                @if (Model.Qty > 0)
                                {
                                    <div class="pro-qty border">
                                        <input min="1" max="100" type="text" value="1">
                                    </div>
                                    <button class="button btn btn-primary" type="submit"><i class="ion-android-add"></i> Add
                                        To Cart
                                    </button>
                                }
                            </div>
                        </div>
                        <div class="product_sku">
                            <p><span>SKU: </span> PRI#@Model.Id</p>
                        </div>
                        <div class="product_tags d-flex">
                            @if (!string.IsNullOrEmpty(Model.Tags))
                            {
                                var tags = Model.Tags.Split(",");
                                <span>tags: </span>
                                <ul class="d-flex">
                                   @foreach (var item in tags)
                                   {
                                       <li><a href="/products/SearchProduct?searchText=@item">@item</a></li>
                                   }
                                </ul>
                            }
                        </div>
                    </form>
                </div>
            </div>
        </div>
    </div>
</section>
<!--product details end-->

